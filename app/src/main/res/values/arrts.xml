<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PageGridView">
        <!--行数，如果为0表示横向显示gridview-->
        <attr name="PagingRows" format="integer" />
        <!--列数,如果为0表示纵向显示gridview-->
        <attr name="PagingColums" format="integer" />
        <attr name="PagingDiver" format="reference" />
    </declare-styleable>

    <declare-styleable name="LineChart">
        <attr name="xLineColor" format="color" />
        <attr name="yLineColor" format="color" />
        <attr name="xPointSpace" format="dimension" />
        <attr name="yPointSpace" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="Mode" format="enum" />
        <attr name="bgColor" format="color" />

    </declare-styleable>
</resources>
